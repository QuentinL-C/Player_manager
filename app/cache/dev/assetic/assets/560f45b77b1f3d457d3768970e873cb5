$(document).ready(function() {
    /* Fonction d'initialisation du player */
    function playerInit() {
        $('#player_gm_navbar_gamename').text('Coucou !');
    }
    
    /* Fonction pour quitter le player */
    function playerQuit() {
        document.location.href="http://www.web.site/";
    }
    
    
    playerInit();
});